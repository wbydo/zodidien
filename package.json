    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "dedent": "^0.7.0",
    "esbuild": "^0.19.0",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fixpack": "^4.0.0",
    "jest": "^29.5.0",
